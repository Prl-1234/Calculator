<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="#008B8B"/>
            <stroke
                android:color="#FFE5B4"
                android:width="1dp"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#2F4F4F"/>
            <stroke
                android:color="#FFE5B4"
                android:width="1dp"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>
</selector>